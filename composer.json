{
  "name": "sfynx-project/cmf-bundle",
  "type": "symfony-bundle",
  "description": "This Bundle provides tools to construct personnal CMS with managers allowed the creation of page with blocks and widgets ( management varnish and memcache with reverse proxy, search lucene manager, render ESI, SEO pages, etc. )",
  "keywords": ["cmf", "ESI", "widget", "cacheHTTP"],
  "homepage": "http://pigroupe.github.io/cmf-sfynx/",
  "license": "MIT",
  "authors": [
    {
      "name": "Etienne de Longeaux",
      "email": "etienne.delongeaux@gmail.com"
    },
    {
      "name": "Sfynx Community",
      "homepage": "https://github.com/pigroupe/SfynxCmfBundle/contributors"
    }
  ],

  "require": {
    "symfony/framework-bundle": "~2.3|~3.0"
  },

  "require-dev": {
    "symfony/phpunit-bridge": "~2.7|~3.0",
    "phpunit/phpunit": "*"
  },

  "suggest": {
  },

  "conflict": {
  },

  "autoload": {
    "psr-0": { "Sfynx\\CmfBundle": "" }
  },

  "target-dir": "Sfynx/CmfBundle",

  "extra": {
    "branch-alias": {
      "dev-master": "2.3-dev"
    }
  }
}
