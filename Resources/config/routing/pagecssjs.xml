<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="admin_pagecssjs" path="/">
        <default key="_controller">SfynxCmfBundle:PageCssJs:index</default>
    </route>

    <route id="admin_pagecssjs_show" path="/{id}/show">
        <default key="_controller">SfynxCmfBundle:PageCssJs:show</default>
    </route>

    <route id="admin_pagecssjs_new" path="/new">
        <default key="_controller">SfynxCmfBundle:PageCssJs:new</default>
    </route>

    <route id="admin_pagecssjs_create" path="/create" methods="POST">
        <default key="_controller">SfynxCmfBundle:PageCssJs:create</default>
    </route>

    <route id="admin_pagecssjs_edit" path="/{id}/edit">
        <default key="_controller">SfynxCmfBundle:PageCssJs:edit</default>
    </route>

    <route id="admin_pagecssjs_update" path="/{id}/update" methods="POST">
        <default key="_controller">SfynxCmfBundle:PageCssJs:update</default>
    </route>

    <route id="admin_pagecssjs_delete" path="/{id}/delete" methods="POST">
        <default key="_controller">SfynxCmfBundle:PageCssJs:delete</default>
    </route>

</routes>
