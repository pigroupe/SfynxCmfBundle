<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="admin_keyword" path="/">
        <default key="_controller">SfynxCmfBundle:KeyWord:index</default>
    </route>

    <route id="admin_keyword_show" path="/{id}/show">
        <default key="_controller">SfynxCmfBundle:KeyWord:show</default>
    </route>

    <route id="admin_keyword_new" path="/new">
        <default key="_controller">SfynxCmfBundle:KeyWord:new</default>
    </route>

    <route id="admin_keyword_create" path="/create" methods="POST">
        <default key="_controller">SfynxCmfBundle:KeyWord:create</default>
    </route>

    <route id="admin_keyword_edit" path="/{id}/edit">
        <default key="_controller">SfynxCmfBundle:KeyWord:edit</default>
    </route>

    <route id="admin_keyword_update" path="/{id}/update" methods="POST">
        <default key="_controller">SfynxCmfBundle:KeyWord:update</default>
    </route>

    <route id="admin_keyword_delete" path="/{id}/delete" methods="POST">
        <default key="_controller">SfynxCmfBundle:KeyWord:delete</default>
    </route>

</routes>
